{
	"name": "spyfallx2",
	"profile": "josh",
	"regions": ["us-east-1"],
	"stages": {
		"staging": {
			"domain": "staging.spyfallx.com",
			"zone": false
		},
		"production": {
			"domain": "spyfallx.com",
			"zone": false
		}
	},
	"lambda": {
		"warm": true,
		"warm_count": 10,
		"policy": [{
			"Effect": "Allow",
			"Resource": "*",
			"Action": [
				"dynamodb:Get*",
				"dynamodb:List*",
				"dynamodb:PutItem",
				"dynamodb:DeleteItem",
				"dynamodb:UpdateItem"
			]
		}]
	}
}
