{
	"compilerOptions": {
		"sourceMap": true,
		"declaration": true,
		"allowUnreachableCode": false,
		"module": "CommonJS",
		"moduleResolution": "node",
		"target": "ESNext",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"noUnusedParameters": true,
		"allowSyntheticDefaultImports": true,
		"strict": false,
		"baseUrl": "./server/",
		"outDir": "./build/",
		"paths": {
			"shared/*": [
				"./shared/*"
			]
		},
		"lib": [
			"es5",
			"es6",
			"es2015",
			"es7",
			"es2016",
			"es2017",
			"es2018",
			"esnext",
			"scripthost",
			"es2015.core",
			"es2015.collection",
			"es2015.generator",
			"es2015.iterable",
			"es2015.promise",
			"es2015.proxy",
			"es2015.reflect",
			"es2015.symbol",
			"es2015.symbol.wellknown",
			"es2016.array.include",
			"es2017.object",
			"es2017.intl",
			"es2017.sharedmemory",
			"es2017.typedarrays",
			"es2018.intl",
			"es2018.promise",
			"es2018.regexp",
			"esnext.asynciterable",
			"esnext.array",
			"esnext.intl",
			"esnext.symbol"
		]
	},
	"typeAcquisition": {
		"enable": true
	},
	"exclude": [
		"node_modules"
	]
}
