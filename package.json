{
  "name": "spyfallx",
  "version": "1.0.0",
  "description": "Spyfall",
  "main": "index.js",
  "scripts": {
    "test": "glob-module-file --pattern=\"+(server|shared)/**/*.test.js\" --format=cjs | node",
    "build:component": "node build-location-map.js && svelte compile component/GameState.html --format=es --output=public/build/game-state.js",
    "build": "tsc -p ./server && npm run build:component",
    "dynamodb": "java -Djava.library.path=~/dynamodb_local_latest/DynamoDBLocal_lib -jar ~/dynamodb_local_latest/DynamoDBLocal.jar -sharedDb",
    "create-schema": "node create-schema.js",
    "dev": "nodemon --exec 'tsc -p ./server' --watch server",
    "start": "node server/index.js",
    "watch-svelte": "watch \"npm run build\" component"
  },
  "keywords": [
    "spyfall"
  ],
  "author": "TehShrike",
  "license": "WTFPL",
  "dependencies": {
    "array-shuffle": "1.0.1",
    "aws-sdk": "2.287.0",
    "combine-arrays": "1.0.2",
    "gamblers-dice": "1.0.1",
    "glob-module-file": "2.2.0",
    "hard-rejection": "1.0.0",
    "just-uuid4": "1.0.0",
    "koa": "^2.5.0",
    "koa-bestest-router": "^1.0.2",
    "koa-body": "4.0.4",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-send": "^4.1.3",
    "loud-rejection": "1.6.0",
    "random-int": "1.0.0",
    "reandom": "0.0.2",
    "seed-random": "2.2.0",
    "sillyname": "0.1.0",
    "svelte": "2.13.2",
    "symlink-cli": "1.0.0",
    "typescript": "3.0.3",
    "zora": "2.0.1"
  },
  "devDependencies": {
    "nodemon": "1.17.5",
    "ts-node": "7.0.1",
    "watch": "1.0.2"
  }
}
